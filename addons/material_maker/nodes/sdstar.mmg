	"shader_model": {
		"code": "",
		"global": [
			"// The MIT License, Copyright Â© 2019 Inigo Quilez",
			"// https://www.shadertoy.com/view/3tSGDy",
			"",
			"float sdStar(vec2 p, float r, int n, float m) {",
			"\tfloat an = 3.141593/float(n);",
			"\tfloat en = 3.141593/m;",
			"\tvec2  acs = vec2(cos(an),sin(an));",
			"\tvec2  ecs = vec2(cos(en),sin(en));",
			"\tfloat bn = mod(atan(p.x,p.y),2.0*an) - an;",
			"\tp = length(p)*vec2(cos(bn),abs(sin(bn)));",
			"\tp -= r*acs;",
			"\tp += ecs*clamp( -dot(p,ecs), 0.0, r*acs.y/ecs.y);",
			"\treturn length(p)*sign(p.x);",
			"}",
			""
		],
		"includes": [
			"sdrotate",
			"sdcirclerepeat"
		],
		"inputs": [

		],
		"instance": "",
		"longdesc": "A regular star described as a signed distance function",
		"name": "sdStar",
		"outputs": [
			{
				"longdesc": "The regular star as a signed distance function",
				"sdf2d": "sdStar($uv - 0.5 - vec2($cx, $cy), $r + $m, int($n), 2.0 + (1.0 - ($m / ($r + $m))) * ($n - 2.0))",
				"shortdesc": "Output",
				"type": "sdf2d"
			}
