{
	"name": "sdroundedbox",
	"node_position": {
		"x": 0,
		"y": 0
	},
	"parameters": {
		"bx": 0.4,
		"by": 0.25,
		"cx": 0.0,
		"cy": 0.0,
		"rw": 0.1,
		"rx": 0.2,
		"ry": 0.2,
		"rz": 0.1
	},
	"seed": 0,
	"seed_locked": false,
	"shader_model": {
		"code": "",
		"global": "float sdRoundedBox( in vec2 p, in vec2 b, in vec4 r )\n{\n    r.xy = (p.x>0.0)?r.xy : r.zw;\n    r.x  = (p.y>0.0)?r.x  : r.y;\n    vec2 q = abs(p)-b+r.x;\n    return min(max(q.x,q.y),0.0) + length(max(q,0.0)) - r.x;\n}",
		"inputs": [

		],
		"instance": "",
		"longdesc": "A rounded box described as a signed distance function",
		"name": "sdRoundedBox",
		"outputs": [
			{
				"longdesc": "The rounded box as a signed distance function",
				"sdf2d": "sdRoundedBox($uv - 0.5 - vec2($cx, $cy), vec2($bx, $by), min(vec4($rx, $ry, $rz, $rw), min($bx, $by)))",
				"shortdesc": "Output",
				"type": "sdf2d"
			}
		],
		"parameters": [
			{
				"control": "Rect1.x",
				"default": 0.15,
				"label": "Width",
				"longdesc": "Width of the rounded box",
				"max": 1,
